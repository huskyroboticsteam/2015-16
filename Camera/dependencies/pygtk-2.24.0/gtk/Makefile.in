install-pygtkPYTHON: $(pygtk_PYTHON)
	@$(NORMAL_INSTALL)
	test -z "$(pygtkdir)" || $(MKDIR_P) "$(DESTDIR)$(pygtkdir)"
	@list='$(pygtk_PYTHON)'; dlist=; list2=; test -n "$(pygtkdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then b=; else b="$(srcdir)/"; fi; \
	  if test -f $$b$$p; then \
	    $(am__strip_dir) \
	    dlist="$$dlist $$f"; \
	    list2="$$list2 $$b$$p"; \
	  else :; fi; \
	done; \
	for file in $$list2; do echo $$file; done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(pygtkdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(pygtkdir)" || exit $$?; \
	done || exit $$?; \
	if test -n "$$dlist"; then \
	  if test -z "$(DESTDIR)"; then \
	    PYTHON=$(PYTHON) $(py_compile) --basedir "$(pygtkdir)" $$dlist; \
	  else \
	    PYTHON=$(PYTHON) $(py_compile) --destdir "$(DESTDIR)" --basedir "$(pygtkdir)" $$dlist; \
	  fi; \
	else :; fi

uninstall-pygtkPYTHON:
	@$(NORMAL_UNINSTALL)
	@list='$(pygtk_PYTHON)'; test -n "$(pygtkdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	filesc=`echo "$$files" | sed 's|$$|c|'`; \
	fileso=`echo "$$files" | sed 's|$$|o|'`; \
	echo " ( cd '$(DESTDIR)$(pygtkdir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(pygtkdir)" && rm -f $$files || exit $$?; \
	echo " ( cd '$(DESTDIR)$(pygtkdir)' && rm -f" $$filesc ")"; \
	cd "$(DESTDIR)$(pygtkdir)" && rm -f $$filesc || exit $$?; \
	echo " ( cd '$(DESTDIR)$(pygtkdir)' && rm -f" $$fileso ")"; \
	cd "$(DESTDIR)$(pygtkdir)" && rm -f $$fileso
install-defsDATA: $(defs_DATA)
