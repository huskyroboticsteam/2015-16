	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(defsdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(defsdir)" || exit $$?; \
	done

